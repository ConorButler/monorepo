{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["plugin:react/recommended", "google"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
        "quotes": [1, "double"],
        "require-jsdoc": 0,
        "object-curly-spacing": 0,
        "react/react-in-jsx-scope": "off",
        "indent": "off"
    }
}
